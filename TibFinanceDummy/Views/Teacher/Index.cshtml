@model TibFinanceDummy.Models.Teacher
@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-md-4">
        @using (Html.BeginForm("Create", "Teacher",
      new { id = "search-form1", @class = "form-horizontal" }, FormMethod.Post))
        {
            <div class="form-group">
                <label asp-for="CustomerName" class="control-label">Customer Name</label>
                @Html.EditorFor(m => m.name)

            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label">Email</label>
                @Html.EditorFor(m => m.email)

            </div>
            <div class="form-group">
                <label asp-for="PhoneNumber" class="control-label">Age</label>
                @Html.EditorFor(m => m.age)

            </div>
            <div class="form-group">
                <input type="submit" id="btnTeacher" value="Create" class="btn btn-primary" />
            </div>

        }
        
    </div>
</div>

